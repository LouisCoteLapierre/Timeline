<Window x:Class="TimelineDataExporter.Windows.MainMenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimelineDataExporter.Windows"
        mc:Ignorable="d"
        Title="Timeline Data Exporter" Height="113.182" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top" Height="0"/>
        <Button Content="View Data" HorizontalAlignment="Center" Margin="0,5,0,5" Grid.Row="1" VerticalAlignment="Center" Click="OnViewDataButtonClicked"/>
   
        <Label Content="Main Menu" HorizontalAlignment="Center" Margin="0,5,0,5" VerticalAlignment="Top" Grid.Row="0"/>
    </Grid>
</Window>
